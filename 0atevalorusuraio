algoritmo "0atevalorusuario"
var
   contador, valor: inteiro
inicio
      contador <- 0
      Escreva("Até qual valor você quer a contagem? ")
      Leia(valor)
      Enquanto (contador<=valor) faca
               Escreval(contador)
               contador <- contador + 1
      FimEnquanto
fimalgoritmo
